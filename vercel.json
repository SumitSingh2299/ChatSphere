{
  "version": 2,
  "builds": [
    {
      "src": "server/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/wsgi.py"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "SECRET_KEY": "@secret_key",
    "FRONTEND_URLS": "@frontend_urls"
  }
}
